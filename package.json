		"react-dom": "^17.0.2",
		"remix": "^1.1.3"
	},
	"devDependencies": {
		"@remix-run/dev": "^1.1.3",
		"cross-env": "^7.0.3",
		"node-sass": "^7.0.1"
	},
	"engines": {
		"node": ">=14"
